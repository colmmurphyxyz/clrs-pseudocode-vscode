  "license": "GPL-3.0-only",
  "displayName": "clrs-pseudocode-vscode",
  "description": "Syntax highlighting for CLRS Pseudocode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "pseudocode",
      "aliases": ["Pseudocode", "pseudocode"],
      "extensions": [".pc"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "pseudocode",
      "scopeName": "source.pseudocode",
      "path": "./syntaxes/pseudocode.tmLanguage.json"
    }]
  }
}
